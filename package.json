{
  "name": "discord-analytics",
  "version": "1.0.0",
  "private": true,
  "description": "Discord Analytics monorepo",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.2",
  "scripts": {
    "dev": "pnpm --parallel --filter './packages/*' dev",
    "build": "pnpm --filter './packages/*' build",
    "start": "pnpm --filter './packages/*' start",
    "lint": "pnpm --filter './packages/*' lint",
    "format": "pnpm --filter './packages/*' format",
    "typecheck": "pnpm --filter './packages/*' typecheck",
    "clean": "pnpm --filter './packages/*' clean",
    "site:dev": "pnpm --filter @discord-analytics/site dev",
    "bot:dev": "pnpm --filter @discord-analytics/bot dev",
    "bot:start": "pnpm --filter @discord-analytics/bot start"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "typescript": "^5.9.2"
  }
}
